{
  "expo": {
    "name": "tiji",
    "slug": "tiji",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicons/playstore.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/appicons/playstore.png",
      "resizeMode": "contain",
      "backgroundColor": "#0034BF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicons/playstore.png",
        "backgroundColor": "#0034BF"
      },
      "package": "com.narcoa47.tiji"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d7aa65e7-08e9-4f6e-8b66-da94e3aafb63"
      }
    }
  }
}
